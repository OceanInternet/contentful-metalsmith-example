    "metalsmith-sass" : {
      "includePaths" : ["bower_components/bootstrap-sass-official/assets/stylesheets/"]
    },
    "metalsmith-assets" : {
      "source" : "assets/",
      "destination" : "assets/"
    }
  }
}
